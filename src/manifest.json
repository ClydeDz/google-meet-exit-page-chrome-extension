{
    "manifest_version": 3,
    "name": "Google Meet Exit Page",
    "description": "Customize the...",
    "version": "0.0.1",
    "author": "Clyde D'Souza",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "options_page": "options.html",
    "permissions": [
      "storage"
    ],
    "content_scripts": [{
      "matches": ["*://meet.google.com/*"],
      "js": ["background.js"]
    }]    
  }
  